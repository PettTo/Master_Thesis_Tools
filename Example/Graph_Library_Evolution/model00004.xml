<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
    <properties/>
    <struct>
        <and abstract="true" mandatory="true" name="GraphLibrary">
            <alt abstract="true" mandatory="true" name="Wgt">
                <feature name="Weighted"/>
                <feature name="Unweighted"/>
            </alt>
            <or abstract="true" name="Algorithms">
                <feature name="Cycle"/>
                <feature name="Number"/>
                <feature name="ShortestPath"/>
                <feature name="Connected"/>
            </or>
            <alt abstract="true" mandatory="true" name="Edges">
                <feature name="Undirected"/>
                <feature name="Directed"/>
            </alt>
            <alt abstract="true" name="Src">
                <feature name="BFS"/>
                <feature name="DFS"/>
            </alt>
        </and>
    </struct>
    <constraints>
        <rule>
            <imp>
                <var>Cycle</var>
                <var>Directed</var>
            </imp>
        </rule>
        <rule>
            <imp>
                <var>ShortestPath</var>
                <conj>
                    <var>Directed</var>
                    <var>Weighted</var>
                </conj>
            </imp>
        </rule>
        <rule>
            <imp>
                <var>Connected</var>
                <conj>
                    <var>Undirected</var>
                    <var>Src</var>
                </conj>
            </imp>
        </rule>
    </constraints>
    <calculations Auto="true" Constraints="true" Features="true" Redundant="true" Tautology="true"/>
    <comments/>
    <featureOrder userDefined="false"/>
</featureModel>
